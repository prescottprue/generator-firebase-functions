{
  "name": "generator-firebase-functions",
  "version": "0.0.1",
  "description": "Generator for Cloud Functions for Firebase",
  "homepage": "",
  "author": {
    "name": "prescottprue",
    "email": "sprue.dev@gmail.com",
    "url": "prue.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "firebase",
    "functions",
    "cloud functions",
    "firebase functions",
    "yeoman",
    "yeoman-generator"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.0",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "istanbul": "^1.1.0-alpha.1",
    "lint-staged": "^4.2.1",
    "mocha": "^3.4.2",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "scripts": {
    "prepare": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "lint": "eslint generators test",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha -R spec ./test/unit/** && npm run test:clean",
    "test:cov": "istanbul cover ./node_modules/mocha/bin/_mocha ./test/unit/** && npm run test:clean",
    "test:clean": "rimraf test/tmp"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/prescottprue/generator-firebase-functions.git",
  "license": "MIT"
}
