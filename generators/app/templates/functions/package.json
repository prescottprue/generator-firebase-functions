{
  "name": "<%= appName %>-functions",
  "description": "Cloud Functions for <%= appName %>",
  "version": "<%= version %>",
  "engines": {
    "node": "6.11.5"
  },
  "scripts": {
    <% if (includeTests) { %>"test": "npm run build && mocha ./test/unit/**",
    "test:cov": "nyc --reporter=lcov npm test",<% } else { %>
    "test": "echo \"No tests included.\"",<% } %>
    "lint": "eslint ./src",
    "build": "npm run clean && babel src -d --out-dir dist",
    "watch": "npm run build -- --watch",
    "clean": "rimraf dist etc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm run build && firebase deploy --only functions",
    "logs": "npm run build && firebase functions:log"
  },
  "dependencies": {
    "firebase-admin": "^8.2.0",
    "firebase-functions": "^3.0.2",
    "glob": "^7.1.3",
    "lodash": "^4.17.11"
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",<% if (includeTests) { %>
    "chai": "^4.1.2",<% } %>
    "cross-env": "^5.1.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^6.9.0",<% if (includeTests) { %>
    "firebase-functions-test": "^0.1.3",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",<% } %>
    "rimraf": "^2.6.2"<% if (includeTests) { %>,
    "sinon": "^4.1.2",<% } %>
    "prettier": "^1.5.3"
  }
}
