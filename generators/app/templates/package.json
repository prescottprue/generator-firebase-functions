{
  "name": "<%= appName %>",
  "version": "0.0.0",
  "description": "Project generated with generator-firebase-functions.",
  "scripts": {
    "build:functions": "npm run functions:build",
    "functions:build": "npm run build --prefix ./functions",
    "functions:test": "npm test --prefix ./functions",
    "functions:serve": "npm run serve --prefix ./functions",
    "functions:start": "npm run start --prefix ./functions",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm i --prefix ./functions",
    "pre-push": "npm run lint"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^6.9.0",
    "husky": "^3.0.1",
    "prettier": "^1.18.2"
  },
  "license": "MIT",
  "homepage": "https://github.com/<%= appName %>/<%= appName %>#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= appName %>/<%= appName %>.git"
  }
}
